muser:TheBeautifulAndTheDamned      rdf:type         muser:Album ;
                                    muser:uuid       "126-abc-qwe" ;
                                    muser:idWikidata "Q123459" ;
                                    muser:idDbpedia  "TbatD" ;
                                    muser:idSpotify  "TheBeaandTheDmnd" ;
                                    muser:name       "The Beautiful And The Damned" ;

select ?entity ?name ?imageURL where {
    ?song rdf:type muser:Song;
          muser:hasMusicalGenre ?songGenre.
    ?songGenre muser:embracedBy ?entity.
    ?entity rdf:type muser:Song;
            muser:name ?name;
            muser:imageURL ?imageURL.
            
    FILTER(?song= {{=it.song}})
    FILTER(?song != ?entity)
} limit 100